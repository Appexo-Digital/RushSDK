source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!
platform :ios, '12.0'
inhibit_all_warnings!

target 'RushSDKSample' do
   pod 'RushSDK', :path => '.'
end

target 'RushSDK' do
  pod 'Alamofire'
  
  pod 'RxSwift', '~> 5.1'
  pod 'RxCocoa', '~> 5.1'
  
  pod 'SwiftyStoreKit'
  
  pod 'FacebookSDK'
  pod 'Amplitude-iOS'
  pod 'Branch'
  pod 'Firebase/Analytics'
  pod 'Firebase/Crashlytics'
  pod 'Firebase/Messaging'
  pod 'AppsFlyerFramework'
end
